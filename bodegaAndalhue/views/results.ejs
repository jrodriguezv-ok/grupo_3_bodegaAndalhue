<!DOCTYPE html>
<html lang="es">

<%- include('partials/head', {title: 'Resultados de tu búsqueda', search: search}) %>

    <body class="fondoBeige">

        <%- include('partials/header')%>


            <h2>Resultados de la búsqueda:
                <%= search %>
            </h2>

            <% if(varietals != undefined){
                for(var i = 0; i < varietals.length; i++){ %>
                <ul>
                    <li>
                        <%= varietals[i].id %>
                    </li>
                    <li>
                        <%= varietals[i].name %>
                    </li>
                </ul>
                <%  } } %>
                    <%- include('partials/footer')%>
    </body>

</html>