    <main>

        <section class="contenedor">
            <h1 class="titulo-seccion">INICIO DE SESIÓN</h1>
            <div class="poesia">¿Estás preparada/o para descubrir un mundo <br>de poesía embotellada?</div>
        </section>
        <div class="slider">
            <img src="/img/slider_register.jpg">
        </div>
        <section class="contenedor">
            <h3 class="enunciado-mayor">Completá tu registro</h3>
            <form class="formLogin" enctype="multipart/form-data" action="javascript:void(0)" method="POST">
                <div class="labelInput">
                    <label for="nombre">Nombre *</label>
                    <input type="text" name="nombre" id="nombre" required>
                </div>
                <div class="labelInput">
                    <label for="apellido">Apellido *</label>
                    <input type="text" name="apellido" id="apellido" required>
                </div>
                <div class="labelInput">
                    <label for="nacimiento">Fecha de Nacimiento *</label>
                    <input type="date" name="nacimiento" id="nacimiento" required>
                </div>
                <div class="labelInput">
                    <label for="direccion">Dirección *</label>
                    <input type="text" name="direccion" id="direccion" required>
                </div>
                <div class="labelInput">
                    <label for="localidad">Localidad *</label>
                    <input type="text" name="localidad" id="localidad" required>
                </div>
                <div class="labelInput">
                    <label for="pais">País *</label>
                    <select id="pais" required>
                    <!-- <option value="" disabled selected>Selecciona tu país</option> -->
                    <option value="argentina">Argentina</option>
                    <option value="brasil">Brasil</option>
                    <option value="bolivia">Bolivia</option>
                    <option value="chile">Chile</option>
                    <option value="colombia">Colombia</option>
                    <option value="ecuador">Ecuador</option>
                    <option value="paraguay">Paraguay</option>
                    <option value="perú">Perú</option>
                    <option value="uruguay">Uruguay</option>
                    </select>
                </div>

                <div class="labelInput">
                    <label for="email">Correo electrónico *</label>
                    <input type="email" name="email" id="email" required>
                </div>
                <div class="labelInput">
                    <label for="password">Contraseña * <a href="">¿Olvidaste tu contraseña?</a></label>
                    <input type="password" name="password" id="password" required>
                </div>
                <button type="submit" id="registro" class="btn-border-max">registrate</button>
            </form>
        </section>
    </main>

